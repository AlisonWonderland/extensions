{
    "name": "TAL Download",
    "version": "1.0",
    "description": "Provides a download link of a 'This American Life' episode on the website",
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png" 
    },
    "content_scripts": [
        {
          "matches": ["https://*.thisamericanlife.org/*"],
          "css": ["styles.css"],
          "js": ["content.js"]
        }
    ],
    "manifest_version": 2
  }